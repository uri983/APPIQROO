<?xml version='1.0' encoding='utf-8'?>
<widget id="com.apiqroo.main.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>APIQROO</name>
    <description>
        Aplicación oficial de la APIQROO.
    </description>
    <author email="programadores@apiqroo.com.mx" href="http://www.apiqroo.com.mx">
        Gerencia de Informática
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
